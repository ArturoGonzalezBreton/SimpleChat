<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <object class="GtkWindow" id="window_ip">
    <property name="title" translatable="1">Chat</property>
    <property name="child">
      <object class="GtkBox" id="box_ip">
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkLabel" id="label_ip">
            <property name="margin-top">12</property>
            <property name="margin-bottom">14</property>
            <property name="label" translatable="1">Direcci√≥n ip:</property>
          </object>
        </child>
        <child>
          <object class="GtkEntry" id="entry_ip">
            <property name="focusable">1</property>
            <property name="overwrite-mode">1</property>
          </object>
        </child>
	<child>
          <object class="GtkLabel" id="label_port">
            <property name="margin-top">12</property>
            <property name="margin-bottom">14</property>
            <property name="label" translatable="1">Puerto:</property>
          </object>
        </child>
	<child>
          <object class="GtkEntry" id="entry_port">
            <property name="focusable">1</property>
            <property name="overwrite-mode">1</property>
          </object>
        </child>
	<child>
          <object class="GtkLabel" id="label_error">
            <property name="margin-top">5</property>
            <property name="margin-bottom">6</property>
            <property name="label" translatable="1"></property>
          </object>
        </child>
        <child>
          <object class="GtkBox" id="box_ip_buttons">
            <child>
              <object class="GtkButton" id="button_cancel_ip">
                <property name="label" translatable="1">Cancelar</property>
                <property name="focusable">1</property>
                <property name="receives-default">1</property>
              </object>
            </child>
            <child>
              <object class="GtkButton" id="button_submit_ip">
                <property name="label" translatable="1">Ingresar</property>
                <property name="focusable">1</property>
                <property name="receives-default">1</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </property>
  </object>
</interface>
